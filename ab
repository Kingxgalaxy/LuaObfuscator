repeat
    wait()
until game:GetService("Players").LocalPlayer and game:GetService("Players").LocalPlayer.Character and game:IsLoaded()
if game.Players.LocalPlayer.UserId == 1524867978 then
Move1 = "Demon Flash"
Move2 = "Blaster Meteor"
Move3 = "Big Bang Kamehameha"
Move4 = "Flash Skewer"
Move5 = "Burning Blast"
Move6 = "Sudden Storm"
Move7 = "Anger Rush"
Move8 = "Flash Strike"
    ---[Settings]---
    DamagePlus = false
    HumanoidAnchored = true
    HeadAnchored = true
    Charge = false
    ChargeTime = whatever
    FOV = 150
    lockon = false
    Earth = false
    form = "g"
    SlowPunch = false
    Grab = 'Dragon Crush'
    ---[Settings]---

    World = {
        536102540,
        3565304751,
        2050207304
    }

    local Div = game.Players.LocalPlayer
    local DivX = Div.Character
    local tpservice = game:service "TeleportService"
    plr = game:GetService("Players").LocalPlayer
    Char = plr.Character
    if Earth == false then
        if game.PlaceId == World[1] then
repeat
    wait()
until game:GetService("Players").LocalPlayer and game:GetService("Players").LocalPlayer.Character and game:IsLoaded()
game:GetService("TeleportService"):Teleport(3565304751, LocalPlayer) end end
    if game.PlaceId == World[2] then
        Rejoin = 20
        DivX:WaitForChild("PowerOutput"):Destroy()wait(0.1)
        game:GetService("RunService").Stepped:connect(
            function()
                DivX.Humanoid:ChangeState(11)
                DivX.HumanoidRootPart.CFrame = CFrame.new(-30, 239, -876)
                Wait(Rejoin)
                tpservice:Teleport(World[2])
            end
        )
    end

    if game.PlaceId == World[3] then
        game:GetService("RunService").RenderStepped:connect(
            function()
                DivX.Humanoid:ChangeState(11)
                if DamagePlus == true then
                    if #game.Players:GetPlayers() >= 2 then
                    
                    end
                end
            end
        )
        wait(1.3)
        game:GetService("RunService").RenderStepped:connect(
            function()
                for i, v in pairs(Div.Character:GetChildren()) do
                    if v.Name == "Attacking" then
                        v:Destroy()
                    end
                    if v.Name == "Action" then
                        v:Destroy()
                    end
                        if v.Name == "hyper" then
                            v:Destroy()
                        end
                            if v.Name == "Using" then
                                v:Destroy()
                            end
                            if v.Name == "MoveStart" then
                                    v:Destroy()
                                end
                            if v.Name == "zDASZzxc" then
                                        v:Destroy()
                            end
                            if v.Name == "KnockBacked" then
                             v:Destroy()
                            end
                end
            end)
        game:GetService'RunService'.Stepped:connect(function()
            for i, v in pairs(game.Workspace.Live['Broly BR']:GetChildren()) do
                if v.Name == "evasive" then
                    v:Destroy()
                end
                if v.Name == "i" then
                    v:Destroy()
                end
                if v.Name == "MoveStart" then
                    v:Destroy()
                end
                if v.Name == "Opos" then
                    v:Destroy()
                end
            end
        end)
        game:GetService("RunService").RenderStepped:connect(
            function()
                if game.Workspace.Live["Broly BR"].Humanoid.Health <= 0 or Div.Humanoid.Health <= 0 then
                    tpservice:Teleport(World[2])
                end
            end
        )
        game:GetService("RunService").RenderStepped:connect(
            function()
                if DivX.Humanoid.Health <= DivX.Humanoid.MaxHealth * 0.2 then
                    Div.Backpack.ServerTraits.Transform:FireServer("g")
                end
            end
        )
        game:GetService("RunService").Stepped:connect(function()
        if game.Players.LocalPlayer.Character.Ki.Value / game.Players.LocalPlayer.Character.Ki.MaxValue * 100 <= 75 then
            game.Players.LocalPlayer.Backpack.ServerTraits.Transform:FireServer(form)
        end
        end)
        end
        wait()
        game:GetService("RunService").Stepped:connect(function()
        game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = CFrame.new(-19, -127, -16)
end)
        Wait()
        game.Workspace.CurrentCamera.FieldOfView = FOV
        wait(.2)
        game.Workspace.CurrentCamera.CFrame = game.Workspace.Live["Broly BR"].Head.CFrame
        for i, v in pairs(game.workspace.Live:GetChildren()) do
            if v:FindFirstChild("Head") and v:FindFirstChild("HumanoidRootPart") then
                v.Head.CFrame = DivX.HumanoidRootPart.CFrame + Vector3.new(-15, 5, 15)
                v.Head.Anchored = HeadAnchored
                v.HumanoidRootPart.Anchored = HumanoidAnchored
                Wait(1)
                game:GetService("RunService").RenderStepped:connect(
            function()
                DivX.Humanoid:EquipTool(Div.Backpack[Grab])
                DivX[Grab]:Activate()
                game.Workspace.Live[DivX.Name][Grab].Activator["Flip"]:Destroy() end)
                wait(.4)
                Div.Backpack.ServerTraits.Assist:FireServer()
                game:GetService("RunService").RenderStepped:connect(
                    function()
                        DivX.HumanoidRootPart.CFrame = CFrame.new(-19.4733, -126.394, -15.8095)
                        Div.Backpack.ServerTraits.EatSenzu:FireServer(true)
                        if DivX.Ki.Value < 50 then
                            Div.Backpack.ServerTraits.Input:FireServer({[1] = "m2"}, CFrame.new(0, 0, 0), nil, SlowPunch)
                        end
                    end
                )
                game:GetService("RunService").Stepped:connect(
                    function()
                        if lockon == true then
                            local BrolyHRP =
                                game:GetService("Workspace").Live["Broly BR"]:FindFirstChild("HumanoidRootPart") or
                                BrolyHumanoid.RootPart
                            local THRP = BrolyHRP.Parent:FindFirstChild("UpperTorso") or BrolyHRP
                            local Camera = game:GetService("Workspace").CurrentCamera
                            Camera.CFrame = CFrame.new(Camera.CFrame.Position, THRP.Position)
                            Camera.FieldOfView = FOV
                        end
                    end
                )
                while wait() do
    for i, v in pairs(Div.Backpack:GetChildren()) do
        if
            v.Name == Move1 or v.Name == Move2 or v.Name == Move3 or v.Name == Move4 or v.Name == Move5 or
                v.Name == Move6 or
                v.Name == Move7 or
                v.Name == Move8
         then
            v.Parent = game:GetService("Workspace").Live[Div.Name]
            wait()
            v:Activate()
            v:Deactivate()
            v.Parent = Div.Backpack
        end
    end
end
end
end
end
